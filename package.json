{
  "name": "matchit",
  "displayName": "matchit",
  "publisher": "redguardtoo",
  "description": "Jump between matching HTML tags and brackets smartly in VS Code. It's ported from Vim matchit by Benji Fisher",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.37.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.matchitJumpItems",
    "onCommand:extension.matchitSelectItems"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.matchitJumpItems",
        "title": "Matchit: Jump Items"
      },
      {
        "command": "extension.matchitSelectItems",
        "title": "Matchit: Select Items"
      }
    ],
    "keybindings": [
      {
        "command": "extension.matchitJumpItems",
        "key": "ctrl+shift+\\",
        "mac": "cmd+shift+\\",
        "when": "editorTextFocus"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/redguardtoo/vscode-matchit"
  },
  "galleryBanner": {
    "color": "#1e423f",
    "theme": "dark"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.37.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "tslint": "^5.12.1",
    "vscode-test": "^1.0.2"
  }
}
